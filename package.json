{
	"name": "rpg-party-inventory",
	"version": "1.2.5",
	"private": true,
	"license": "MIT",
	"scripts": {
		"start:dev": "next dev",
		"dev-db:wait": "wait-on tcp:5432 && delay 0.4",
		"dev-db:run": "docker-compose up",
		"dev-db:start": "concurrently \"yarn dev-db:run\" \"yarn dev-db:wait && yarn prisma migrate dev\"",
		"dev-db:reset": "docker-compose down --rmi all --volumes",
		"dev-db:studio": "yarn prisma studio",
		"build": "next build",
		"start": "next start",
		"test:all": "yarn test:unit && yarn test:integration",
		"test:unit": "jest --runInBand",
		"test:frontend": "jest --runInBand --selectProjects frontend",
		"test:backend": "jest --runInBand --selectProjects backend",
		"test:integration": "playwright test",
		"setupDevDb": "ts-node scripts/setupDevDb.ts",
		"commit": "git cz",
		"check-types": "tsc --noEmit",
		"release": "standard-version",
		"release:patch": "yarn release --release-as patch",
		"release:minor": "yarn release --release-as minor",
		"release:major": "yarn release --release-as major",
		"lint": "eslint --fix src tests scripts",
		"prepare": "husky install && playwright install",
		"postinstall": "prisma generate",
		"clean-deps": "yarn flush-deps && yarn",
		"flush-deps": "npx rimraf node_modules",
		"ci": "yarn install --freeze-lockfile",
		"prisma": "dotenv -e .env.development prisma"
	},
